{
  "name": "@full/core",
  "version": "0.1.0",
  "description": "FULL.js core framework package",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist",
