{
  "name": "@full/testing",
  "version": "0.1.0",
  "description": "Testing utilities for FULL.js applications",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:performance": "vitest performance",
    "lint": "eslint src --ext .ts,.tsx",
    "format": "prettier --write src"
  },
  "dependencies": {
    "@full/core": "workspace:*",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "chrome-launcher": "^1.0.0",
    "lighthouse": "^11.0.0",
    "playwright": "^1.35.0",
    "vitest": "^0.32.0"
  },
  "devDependencies": {
    "@types/chrome-launcher": "^0.13.2",
    "@types/lighthouse": "^9.6.1",
    "@types/node": "^20.3.1",
    "@types/react": "^18.2.12",
    "@types/react-dom": "^18.2.5",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "@vitest/coverage-v8": "^0.32.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-react": "^7.32.2",
    "prettier": "^2.8.8",
    "typescript": "^5.1.3"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
